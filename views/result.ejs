<% include partials/header %>

<h1>Movie Details</h1>

<a href="/" id="top">Search again</a>

<div class="result">
    <div class="list">
       <% for(var i=0;i<dataPasrsed["Search"].length;i++){ %>
       
            <% var temp = dataPasrsed["Search"][i]; %>
            <p id="border"></p>
            <strong><li>Title: <%= temp["Title"] %> </li></strong>
            <li>Year: <%= temp["Year"] %> </li>
            <li>IMDB-ID: <%= temp["imdbID"] %> </li>
            <li>Year: <%= temp["Year"] %> </li>
            <% if(temp["Poster"]=== "N/A"){ %>
                <li class="poster">Poster isn't Available </li>
                <% } else{ %>
                    <li><img src = "<%=temp["Poster"] %>" > </li>
                <% } %>
           <% } %>
        </div>
</div>

<a href="#top">Top</a>
